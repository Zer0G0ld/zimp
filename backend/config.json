{
"snmp": {
    "community": "public",
    "version": "2c",
    "timeout": 1,
    "retries": 0
},
"network_scan": {
    "network": "192.168.8.0/24",
    "ping_timeout_ms": 1000,
    "max_parallel": 20
},
"logging": {
    "enabled": true,
    "level": "info",
    "file_path": "scanner.log"
},
"output": {
    "enabled": true,
    "format": "json",
    "file_path": "devices_found.json"
}
}
